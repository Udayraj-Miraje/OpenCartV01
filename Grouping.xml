    <classes>
     		<class name="testCases.TC01_NewAccRegistration"/> 
    		<class name="testCases.TC02_AccLogin"/>
    	  <!-- <class name= "testCases.TC03_AccLoginWithDDT"/> -->
    </classes>
  </test> <!-- MasterTest -->
</suite> <!-- MasterSuite -->
